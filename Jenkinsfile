pipeline {
     agent any
     stages {
          stage('Init'){
                steps {
                      echo "This is sowmiya learning DevOps"
                      echo "Try to script also"
                      }
           }
          stage('Build'){
                steps {
                      echo "Building sample code pipeline"
                }               
          }
         stage('deploy stage'){
                steps {
                      echo "Deployed in Stage Environment"
                }
          }
          stage('deploy prod'){
                steps {
                      echo "Deployed in Production Environment"
                }
          }
     }
}

    
